{
    "cmake.sourceDirectory": "${workspaceFolder}/src",
    "C_Cpp.default.configurationProvider": "ms-vscode.cmake-tools",
    "cmake.configureSettings": {
        "ENABLE_PAYLOAD_LOGGING": "OFF",
        "COMPILE_WITH_STRICTNESS": "ON",
        "use_prov_client": "ON",
        "hsm_type_symm_key": "ON"
    },
    // Enables C# XML Auto Commenting
    "editor.formatOnType": true,
    // Use JSON with comments
    "files.associations": {
        "*.json": "jsonc",
        "string": "cpp",
        "any": "cpp",
        "optional": "cpp",
        "cctype": "cpp",
        "clocale": "cpp",
        "cmath": "cpp",
        "cstdarg": "cpp",
        "cstddef": "cpp",
        "cstdio": "cpp",
        "cstdlib": "cpp",
        "cstring": "cpp",
        "ctime": "cpp",
        "cwchar": "cpp",
        "cwctype": "cpp",
        "array": "cpp",
        "atomic": "cpp",
        "bit": "cpp",
        "*.tcc": "cpp",
        "bitset": "cpp",
        "chrono": "cpp",
        "cinttypes": "cpp",
        "compare": "cpp",
        "complex": "cpp",
        "concepts": "cpp",
        "condition_variable": "cpp",
        "csetjmp": "cpp",
        "csignal": "cpp",
        "cstdint": "cpp",
        "deque": "cpp",
        "list": "cpp",
        "map": "cpp",
        "set": "cpp",
        "unordered_map": "cpp",
