{
  "@context": "dtmi:dtdl:context;3",
  "@id": "dtmi:osconfig:pmc;1",
  "@type": "Interface",
  "displayName": "Package Manager Configuration",
  "description": "Module designed to install DEB-packages using Package Manager",
  "contents": [
    {
      "@type": "Property",
      "name": "state",
      "schema": {
        "@type": "Object",
        "fields": [
          {
            "name": "packages",
            "schema": {
              "@type": "Array",
              "elementSchema": "string"
            }
          },
          {
            "name": "packagesFingerprint",
            "schema": "string"
          },
          {
            "name": "sourcesFingerprint",
            "schema": "string"
          },
          {
            "name": "sourcesFilenames",
            "schema": {
              "@type": "Array",
              "elementSchema": "string"
            }
          },
          {
            "name": "executionState",
            "schema": {
              "@type": "Enum",
              "valueSchema": "integer",
              "enumValues": [
                {
                  "name": "unknown",
                  "enumValue": 0
                },
                {
                  "name": "running",
                  "enumValue": 1
                },
                {
                  "name": "succeeded",
                  "enumValue": 2
                },
                {
                  "name": "failed",
                  "enumValue": 3
                },
                {
                  "name": "timedOut",
                  "enumValue": 4
                }
              ]
            }
          },
          {
            "name": "executionSubstate",
            "schema": {
              "@type": "Enum",
              "valueSchema": "integer",
              "enumValues": [
                {
                  "name": "none",
                  "enumValue": 0
                },
                {
                  "name": "deserializingJsonPayload",
                  "enumValue": 1
                },
                {
                  "name": "deserializingDesiredState",
                  "enumValue": 2
                },
                {
                  "name": "deserializingPackages",
                  "enumValue": 3
                },
                {
                  "name": "deserializingGpgKeys",
                  "enumValue": 4
                },
                {
                  "name": "deserializingSources",
                  "enumValue": 5
                },
                {
                  "name": "downloadingGpgKeys",
                  "enumValue": 6
                },
                {
                  "name": "modifyingSources",
                  "enumValue": 7
                },
                {
                  "name": "updatingPackageLists",
                  "enumValue": 8
                },
                {
                  "name": "installingPackages",
                  "enumValue": 9
                }
              ]
            }
          },
          {
            "name": "executionSubstateDetails",
            "schema": "string"
          }
        ]
      },
      "writable": false
    },
    {
      "@type": "Property",
      "name": "desiredState",
      "schema": {
        "@type": "Object",
        "fields": [
          {
            "name": "packages",
            "schema": {
              "@type": "Array",
              "elementSchema": "string"
            }
          },
          {
            "name": "sources",
            "schema": {
              "@type": "Map",
              "mapKey": {
                "name": "id",
                "schema": "string"
              },
              "mapValue": {
                "name": "entry",
                "schema": "string"
              }
            }
          },
          {
            "name": "gpgKeys",
            "schema": {
              "@type": "Map",
              "mapKey": {
                "name": "id",
                "schema": "string"
              },
              "mapValue": {
                "name": "url",
                "schema": "string"
              }
            }
          }
        ]
      },
      "writable": true
    }
